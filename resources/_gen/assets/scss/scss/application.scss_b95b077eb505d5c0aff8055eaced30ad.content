*,*::before,*::after{box-sizing:border-box}body,h1,h2,h3,h4,h5,h6,p,figure,blockquote,dl,dd{margin:0}ul,ol{padding:0}html{scroll-behavior:smooth;font-family:sans-serif}body{line-height:1.5;min-height:100vh;text-rendering:optimizeSpeed;-webkit-font-smoothing:antialiased}ul,ol{list-style:none}a{text-decoration:none;color:inherit}img{max-width:100%;display:block}input,button,textarea,select{font:inherit}html,body{height:100%}@font-face{font-family:timegoing;src:url(/fonts/Timegoing-Regular.ttf)format("ttf");font-weight:400;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Light.ttf)format("ttf");font-weight:light;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Regular.ttf)format("ttf");font-weight:400;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Medium.ttf)format("ttf");font-weight:medium;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-SemiBold.ttf)format("ttf");font-weight:600;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Bold.ttf)format("ttf");font-weight:700;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Italic.ttf)format("ttf");font-weight:400;font-style:italic}@font-face{font-family:poppins;src:url(/fonts/Poppins-LightItalic.ttf)format("ttf");font-weight:light;font-style:italic}body{background-color:#f5f3f1}.focus__container{display:flex;justify-content:center;min-height:100vh;min-width:100vw;padding:4rem}.focus__area{position:relative;flex:1;border:.1875rem solid #141009;transform-origin:center;transition:.8s cubic-bezier(.25,.1,.25,1)}.focus__area::after{content:"";position:absolute;top:50%;left:-.1875rem;width:calc(100% + .1875rem * 2);height:9.375rem;background-color:#f5f3f1}.focus__area.active{flex:none;width:calc(100vw/3)}