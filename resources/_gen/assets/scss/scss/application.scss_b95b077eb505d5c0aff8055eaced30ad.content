*,*::before,*::after{box-sizing:border-box}body,h1,h2,h3,h4,h5,h6,p,figure,blockquote,dl,dd{margin:0}ul,ol{padding:0}html{scroll-behavior:smooth;font-family:sans-serif}body{line-height:1.5;min-height:100vh;text-rendering:optimizeSpeed;-webkit-font-smoothing:antialiased}ul,ol{list-style:none}a{text-decoration:none;color:inherit}img{max-width:100%;display:block}input,button,textarea,select{font:inherit}html,body{height:100%}@font-face{font-family:timegoing;src:url(/fonts/Timegoing-Regular.ttf)format("ttf");font-weight:400;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Light.ttf)format("ttf");font-weight:light;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Regular.ttf)format("ttf");font-weight:400;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Medium.ttf)format("ttf");font-weight:medium;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-SemiBold.ttf)format("ttf");font-weight:600;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Bold.ttf)format("ttf");font-weight:700;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-ExtraBold.ttf)format("ttf");font-weight:800;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Italic.ttf)format("ttf");font-weight:400;font-style:italic}@font-face{font-family:poppins;src:url(/fonts/Poppins-LightItalic.ttf)format("ttf");font-weight:light;font-style:italic}body{background-color:#f5f3f1}.home{position:relative;justify-content:space-between;padding:4rem;flex:1}.home__content-container{flex:1;display:flex;flex-direction:column;gap:1rem;padding:1.5rem 0 2rem;text-align:center;font-family:poppins,sans-serif}.home__content-container .home__titles{display:flex;flex-direction:column;gap:.75rem}.home__content-container .special-title{font-size:10rem;line-height:.9em;font-family:timegoing,sans-serif;font-weight:400;transform:rotate(355deg);text-shadow:0 -.5625rem 0 #f5f3f1,.625rem 0 0 #f5f3f1,0 .625rem 0 #f5f3f1,-.3125rem 0 0 #f5f3f1}.home__content-container .special-subtitle{z-index:100;position:relative;font-size:4rem;font-weight:600;text-transform:uppercase;line-height:.9em}.home__content-container .special-subtitle::before{z-index:-1;content:"";position:absolute;top:0;left:0;width:100%;height:100%;background-color:#f5f3f1}.home__content-container .home__content{display:flex;flex-direction:column;flex:1}.home__content-container .job-details span{font-weight:300;font-size:1.25rem;text-transform:uppercase}.home__content-container .name{display:flex;flex-direction:column;margin-top:auto}.home__content-container .name .firstname,.home__content-container .name .lastname{text-transform:uppercase}.home__content-container .name .firstname{font-size:1.125rem;font-weight:500}.home__content-container .name .lastname{font-size:1.5rem;font-weight:800}.home__side{display:flex;flex-direction:column;justify-content:space-between;position:fixed;left:0;top:0;bottom:0;width:fit-content;padding:4rem 0;background-color:#f5f3f1}.home__side .bottom{writing-mode:vertical-rl}.home__side .bottom .last-job{transform:rotate(180deg);display:flex;flex-direction:column;align-items:center;gap:.75rem}.home__side .bottom .last-job p{font-size:.875rem;text-transform:uppercase}.focus__container{position:fixed;top:0;left:0;z-index:-1;display:flex;justify-content:center;min-height:100vh;min-width:100vw;padding:4rem}.focus__area{position:relative;width:calc(100vw - 64px * 2);border:.1875rem solid #141009;transform-origin:center;transition:.8s cubic-bezier(.25,.1,.25,1)}.focus__area::after{content:"";position:absolute;top:65%;left:50%;transform:translateX(-50%);width:calc(100% + .1875rem * 9);height:9.375rem;background-color:#f5f3f1}.focus__area.active{border-left:calc(.1875rem * 3)solid #141009;border-right:calc(.1875rem * 3)solid #141009;transform:scaleX(.33)}.portfolio{height:300vh}.outer{overflow-x:hidden}.slider{display:flex;width:200vw}.slider section{height:100vh;width:50%;padding:4rem}.slider section .inner{position:relative;display:flex;height:100%}.slider section .inner .content{background-color:#f5f3f1}.slider section:nth-child(odd) .inner{align-items:start;transform:translate(3rem,-1.5rem)}.slider section:nth-child(even) .inner{align-items:end;transform:translate(2rem,-4rem)}.canvas-container{z-index:0;width:100%;height:250px;position:relative;background-color:#141009;outline:.6875rem solid #f5f3f1}canvas{width:100%;height:100%}.project__title{font-size:4rem;font-family:timegoing,sans-serif;transform:rotate(357deg);transform-origin:left;line-height:1em;background-color:#f5f3f1;width:calc(100% + 1rem);padding:.75rem 1.25rem 0 0}.site-content{z-index:1000;display:flex;flex-direction:column;min-height:100vh;min-width:100vw}.site-content__section{display:none}.site-content__section.active{display:flex}