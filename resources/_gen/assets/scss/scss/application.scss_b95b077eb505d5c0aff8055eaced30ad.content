*,*::before,*::after{box-sizing:border-box}body,h1,h2,h3,h4,h5,h6,p,figure,blockquote,dl,dd{margin:0}ul,ol{padding:0}html{scroll-behavior:smooth;font-family:sans-serif}body{line-height:1.5;min-height:100vh;text-rendering:optimizeSpeed;-webkit-font-smoothing:antialiased}ul,ol{list-style:none}a{text-decoration:none;color:inherit}img{max-width:100%;display:block}input,button,textarea,select{font:inherit}html,body{height:100%}@font-face{font-family:street-font;src:url(/fonts/Timegoing-Regular.ttf)format("truetype");font-weight:400;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Light.ttf)format("truetype");font-weight:light;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Regular.ttf)format("truetype");font-weight:400;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Medium.ttf)format("truetype");font-weight:medium;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-SemiBold.ttf)format("truetype");font-weight:600;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Bold.ttf)format("truetype");font-weight:700;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-ExtraBold.ttf)format("truetype");font-weight:800;font-style:normal}@font-face{font-family:poppins;src:url(/fonts/Poppins-Italic.ttf)format("truetype");font-weight:400;font-style:italic}@font-face{font-family:poppins;src:url(/fonts/Poppins-LightItalic.ttf)format("truetype");font-weight:light;font-style:italic}body{background-color:#f5f3f1}.home{position:relative;justify-content:space-between;padding:clamp(2rem,8vw,4rem);flex:1}.home__content-container{flex:1;display:flex;flex-direction:column;gap:clamp(.5rem,2vw,1rem);padding:clamp(.75rem,3vw,1.5rem)0 clamp(1rem,4vw,2rem);text-align:center;font-family:poppins,sans-serif}.home__content-container .home__titles{display:flex;flex-direction:column;align-items:center;gap:clamp(.375rem,1.5vw,.75rem)}.home__content-container .special-title{font-size:clamp(2rem,10vw,10rem);line-height:.9em;font-family:street-font,sans-serif;font-weight:400;transform:rotate(355deg);text-shadow:0 -.5625rem 0 #f5f3f1,.625rem 0 0 #f5f3f1,0 .625rem 0 #f5f3f1,-.3125rem 0 0 #f5f3f1}.home__content-container .special-subtitle{z-index:100;position:relative;font-size:clamp(1.5rem,6vw,4rem);font-weight:600;text-transform:uppercase;line-height:.9em;width:fit-content}.home__content-container .special-subtitle::before{z-index:-1;content:"";position:absolute;top:0;left:0;width:100%;height:100%;background-color:#f5f3f1}.home__content-container .home__content{display:flex;flex-direction:column;align-items:center;flex:1}.home__content-container .job-details span{font-weight:300;font-size:clamp(1rem,3vw,1.25rem);text-transform:uppercase}.home__content-container .logo{margin-top:clamp(.75rem,3vw,1.5rem);width:clamp(.875rem,4vw,4rem);opacity:0}.home__content-container .name{display:flex;flex-direction:column;margin-top:clamp(1rem,4vw,2rem)}.home__content-container .name .firstname,.home__content-container .name .lastname{text-transform:uppercase}.home__content-container .name .firstname{font-size:clamp(.875rem,2.5vw,1.125rem);font-weight:500}.home__content-container .name .lastname{font-size:clamp(1.125rem,4vw,1.5rem);font-weight:800}.home__side{display:flex;flex-direction:column;justify-content:space-between;align-items:center;position:fixed;left:clamp(.625rem,2.5vw,1.25rem);top:0;bottom:0;width:fit-content;padding:clamp(2rem,8vw,4rem)0}.home__side .middle{width:clamp(.875rem,2.5vw,1.125rem)}.home__side .top,.home__side .bottom{writing-mode:vertical-rl}.home__side .cv{transform:rotate(180deg);display:flex;flex-direction:row;align-items:center;gap:clamp(.375rem,1.5vw,.75rem)}.home__side .cv p{font-size:clamp(.625rem,1.5vw,.75rem);font-weight:300;text-transform:uppercase;height:28ch}.focus__container{position:fixed;top:0;left:0;z-index:-1;display:flex;justify-content:center;min-height:100vh;min-width:100vw;padding:clamp(2rem,8vw,4rem)}.focus__area{position:relative;width:calc(100vw - 64px * 2);border:.1875rem solid #141009;transform-origin:center;transition:.8s cubic-bezier(.25,.1,.25,1)}.focus__area::after{content:"";position:absolute;top:65%;left:50%;transform:translateX(-50%);width:calc(100% + .1875rem * 9);height:9.375rem;background-color:#f5f3f1}.focus__area.active{border-left:calc(.1875rem * 3)solid #141009;border-right:calc(.1875rem * 3)solid #141009;transform:scaleX(.33)}.portfolio{height:300vh}.outer{overflow-x:hidden}.slider{display:flex;width:200vw}.slider section{height:100vh;width:50%;padding:clamp(2rem,8vw,4rem)}.slider section .inner{position:relative;display:flex;height:100%}.slider section .inner .content{background-color:#f5f3f1}.slider section:nth-child(odd) .inner{align-items:start;transform:translate(clamp(1.5rem,6vw,3rem),clamp(-.75rem,-3vw,-1.5rem))}.slider section:nth-child(even) .inner{align-items:end;transform:translate(clamp(1rem,4vw,2rem),clamp(-2rem,-8vw,-4rem))}.project__title{font-size:clamp(1.5rem,6vw,4rem);font-family:street-font,sans-serif;font-weight:400;transform:rotate(357deg)translateY(-10px);transform-origin:left;line-height:1em;background-color:#f5f3f1;width:calc(100% + clamp(.5rem,2vw,1rem));padding:clamp(.375rem,1.5vw,.75rem)clamp(.625rem,2.5vw,1.25rem)0 0}.project__btn{position:absolute;top:1.25rem;right:1.25rem;padding:clamp(.25rem,1vw,.5rem);border-radius:1.25rem;background-color:#f5f3f1;transition:.3s ease-in-out}.project__btn img{width:1.125rem;transform:rotate(45deg);transition:transform .3s ease-in-out}.project__btn:hover{opacity:.8;scale:1.15}.project__btn:hover img{width:1.125rem;transform:rotate(0)}.canvas-container{z-index:0;height:fit-content;position:relative;outline:.6875rem solid #f5f3f1}canvas{width:100%;height:100%}.site-content{z-index:1000;display:flex;flex-direction:column;min-height:100vh;min-width:100vw}.site-content__section{display:none}.site-content__section.active{display:flex}.site-content__section.portfolio{transform:translateY(-8em)}.intro{position:absolute;top:50%;left:50%;transform:translate(-50%,-50%);font-size:clamp(1.5rem,6vw,4rem);font-family:street-font,sans-serif;font-weight:400;animation:loader 1s alternate-reverse infinite ease-in-out}@keyframes loader{0%{opacity:.4}100%{opacity:.8}}